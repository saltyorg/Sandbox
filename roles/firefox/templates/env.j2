USER_ID={{ uid }}
GROUP_ID={{ gid }}
LANG={{ system_locale }}
TZ={{ tz }}
KEEP_APP_RUNNING=0
APP_NICENESS=0
INSTALL_PACKAGES=
CONTAINER_DEBUG=0
DISPLAY_WIDTH=1920
DISPLAY_HEIGHT=1080
DARK_MODE=1
{% if firefox_web_port != '5800' %}
WEB_LISTENING_PORT={{ firefox_web_port }}
{% endif %}
{% if firefox_vnc_port != '5900' %}
VNC_LISTENING_PORT={{ firefox_vnc_port }}
{% endif %}
ENABLE_CJK_FONT=0
FF_OPEN_URL=
FF_KIOSK=0
FF_PREF_DOWNLOADS=browser.download.dir="{{ firefox_paths_downloads_location }}"
