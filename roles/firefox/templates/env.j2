USER_ID={{ uid }}
GROUP_ID={{ gid }}
LANG={{ system_locale }}
TZ={{ tz }}
KEEP_APP_RUNNING=0
APP_NICENESS=0
INSTALL_PACKAGES=
CONTAINER_DEBUG=0
DISPLAY_WIDTH=1920
DISPLAY_HEIGHT=1080
DARK_MODE=1
{% if lookup('role_var', '_web_port', role='firefox') != '5800' %}
WEB_LISTENING_PORT={{ lookup('role_var', '_web_port', role='firefox') }}
{% endif %}
{% if lookup('role_var', 'vnc_port', role='firefox') != '5900' %}
VNC_LISTENING_PORT={{ lookup('role_var', 'vnc_port', role='firefox') }}
{% endif %}
ENABLE_CJK_FONT=0
FF_OPEN_URL=
FF_KIOSK=0
FF_PREF_DOWNLOADS=browser.download.dir="{{ lookup('role_var', '_paths_downloads_location', role='firefox') }}"
